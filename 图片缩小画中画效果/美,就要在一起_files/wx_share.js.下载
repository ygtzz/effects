var wxObj={wx_share:function(n){if(null!==n){var e=/MicroMessenger/.test(window.navigator.userAgent);if(config={url:location.href.split("#")[0],noncestr:Math.random().toString(36).substr(2),timestamp:(new Date).getTime()},e){new Promise(function(n,e){$.ajax({type:"POST",url:"/wx/signature",data:config,dataType:"json",success:function(e){n(e.data)},error:function(n){e(n)}})}).then(function(n){wx.config({debug:!1,jsApiList:["onMenuShareAppMessage","onMenuShareTimeline","onMenuShareQQ"],appId:n.appId,timestamp:n.timestamp,nonceStr:n.nonceStr,signature:n.signature})}).catch(function(n){});wx.ready(function(){wx.onMenuShareAppMessage(n),wx.onMenuShareTimeline(n),wx.onMenuShareQQ(n)})}}}};